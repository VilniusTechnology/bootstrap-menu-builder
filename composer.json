{
    "name": "vilnius-technology/bootstrap-menu-builder",
    "description": "This is an OOP way to build bootstrap menu, with nested sub-menus.",
    "keywords": ["bootstrap", "oop", "php"],
    "license": "MIT",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "phpmd/phpmd" : "@stable",
        "sebastian/phpcpd": "~2.0",
        "sebastian/phpdcd": "~1.0",
        "phploc/phploc": "~2.0",
        "ongr/ongr-strict-standard": "2.*"
    },
    "autoload": {
        "psr-4": {
            "VilniusTechnology\\BootstrapMenuBuilder\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
